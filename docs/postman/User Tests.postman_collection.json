      "name": "createUser",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code should be 201\", () => {\r",
              "    pm.response.to.have.status(201);\r",
              "})\r",
              "\r",
              "const id = pm.response.json().id;\r",
              "\r",
              "pm.environment.set(\"id\", id);\r",
              "\r",
              "pm.test(\"Response should have correct body\", () => {\r",
              "    const body = pm.response.json();\r",
              "    pm.expect(body).to.have.property(\"id\").and.not.null\r",
              "    pm.expect(body).to.have.property(\"name\").and.equal(pm.collectionVariables.get(\"name\"))\r",
              "    pm.expect(body).to.have.property(\"email\").and.equal(pm.collectionVariables.get(\"email\"))\r",
              "    pm.expect(body).to.have.property(\"gender\").and.equal(pm.collectionVariables.get(\"gender\"))\r",
              "    pm.expect(body).to.have.property(\"status\").and.equal(pm.collectionVariables.get(\"status\"))\r",
              "})"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
